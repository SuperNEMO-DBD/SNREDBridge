#ifndef SNREDBRIDGE_SNREDBRIDGE_CONFIG_H
#define SNREDBRIDGE_SNREDBRIDGE_CONFIG_H

//! Evaluates to true if SNREDBridge was built with documentation
#cmakedefine01 SNREDBRIDGE_WITH_DOCS

//! Evaluates to true if SNREDBridge was built with documentation
#cmakedefine01 SNREDBRIDGE_WITH_PLUGINS

//! Evaluates to true if SNREDBridge was built with service support
#cmakedefine01 SNREDBRIDGE_WITH_SERVICE

//! Evaluates to true if SNREDBridge was built with Bayeux dependency
#cmakedefine01 SNREDBRIDGE_WITH_BAYEUX_DEPENDENCY

#endif // SNREDBRIDGE_SNREDBRIDGE_CONFIG_H

